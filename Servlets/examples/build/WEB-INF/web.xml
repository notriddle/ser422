<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2.2.dtd">

<web-app>
    <display-name>Java Class Examples</display-name>

    <servlet>
        <servlet-name>DemoSiteSelector</servlet-name>
        <servlet-class>DemoSiteSelector</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>EditCookies</servlet-name>
        <servlet-class>EditCookies</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DemoSiteSelector</servlet-name>
        <url-pattern>redirect/*</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>EditCookies</servlet-name>
        <url-pattern>cookies/*</url-pattern>
    </servlet-mapping>
    
</web-app>
